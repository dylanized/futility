<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title><%= data.siteName %> :: <%= data.siteTagline %></title>
    <% stylesheets.forEach((stylesheet) => { %>
      <link rel="stylesheet" href="<%= stylesheet %><% if (typeof req !== 'undefined' && req.query.flavor) { %>?flavor=<%= req.query.flavor %><% } %>">
    <% }); %>
    <link rel="icon" href="./favicon.ico" type="image/x-icon">
  </head>
  <body class="p-2">
