<%- include('inc/head') %>
<main class="container">
  <section>
    <h1>Futility Stylesets</h1>
    <div class="mb-2 d-grid cols-4 col-gap-3">
      <% for (const styleset_slug in stylesets) { %>
        <div class="text-verdana panel border-<%= styleset_slug %> bg-white rounded p-05 mb-2 text-center" style="min-height:180px;">
          <div class="px-1 py-3 <%= styleset_slug %> rounded mb-05">
            <div class="text-bold clippy">.<%= styleset_slug %></div>
            <div class="text-xs clippy">.bg-<%= styleset_slug %></div>
          </div>
          <div class="text-bold text-sm mb-0 text-<%= styleset_slug %> clippy lh-12">.text-<%= styleset_slug %></div>
          <div class="text-xs text-<%= styleset_slug %> clippy mb-05 lh-11">.border-<%= styleset_slug %></div>
        </div>
      <% } %>
    </div>
    <p><a title="Futility UI Contents" href="/contents">&larr; Back to Contents</a></p>
  </section>
</main>
<%- include('inc/foot') %>
