{
  "name": "minimos",
  "version": "1.0.0",
  "description": "lightweight ui framework",
  "main": "index.js",
  "scripts": {
    "build": ". bin/build",
    "clean": "rimraf dist/",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
